	<queryString>
		<![CDATA[SELECT
     MAS_HS_Audits."Year" AS MAS_HS_Audits_Year,
     MAS_HS_Audits."Month" AS MAS_HS_Audits_Month,
     MAS_HS_Audits."Customer" AS MAS_HS_Audits_Customer,
     MAS_HS_Audits."Style" AS MAS_HS_Audits_Style,
     MAS_HS_Audits."Graphic" AS MAS_HS_Audits_Graphic,
     MAS_HS_Audits."Combo" AS MAS_HS_Audits_Combo,
     sum(MAS_HS_Audits."Lot_Size") AS MAS_HS_Audits_Lot_Size,
     sum(MAS_HS_Audits."Sam_Size") AS MAS_HS_Audits_Sam_Size,
     sum(MAS_HS_Audits."Def_QTY") AS MAS_HS_Audits_Def_QTY,
     MAS_HS_Audits."Def_Type" AS MAS_HS_Audits_Def_Type,
     avg(MAS_HS_Audits."Def_Pec") AS MAS_HS_Audits_Def_Pec,
     MAS_HS_Audits."Aud_Comment" AS MAS_HS_Audits_Aud_Comment
FROM
     "dbo"."MAS_HS_Audits" MAS_HS_Audits
WHERE
     $X{ IN ,Year,list}
     and $X{ IN ,Month,list}
     and $X{ IN ,Customer,list}
     and $X{ IN ,Style,list}
     and $X{ IN ,Graphic,list}
     and $X{ IN ,Combo,list}
GROUP BY
     MAS_HS_Audits.Year,
     MAS_HS_Audits.Month,
     MAS_HS_Audits.Customer,
     MAS_HS_Audits.Style,
     MAS_HS_Audits.Graphic,
     MAS_HS_Audits.Combo,
     MAS_HS_Audits.Def_Type,
     MAS_HS_Audits.Aud_Comment]]>
	</queryString>
