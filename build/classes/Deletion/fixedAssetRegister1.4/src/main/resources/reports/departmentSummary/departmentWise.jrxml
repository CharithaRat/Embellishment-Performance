    <queryString><![CDATA[SELECT
   dm.DEP_CODE,
  dm.DEP_DES,
  lm.LOC_DES,
 ald.LOC_ID, SUM(ASSET_VALUE) AS addition,
   BF_DISP_VALUE AS blcFord,
  IFNULL(SUM(DISPOSAL_AMOUNT),0) AS disposal,
  SUM(TO_DATE_DISP_VAL) AS totdep,
  IFNULL(SUM(DISP_VALUE_DISPOSAL),0) AS Depval,
  RPD.START_DATE,
  RPD.END_DATE,
  RPD.CURRENT_VALUE AS WDE,RPD.REGISTED_DATE ,RPD.ASSET_VALUE,SUM(FROM_DATE_DISP_VAL) AS DepAtFdate,
  SUM(IF(RPD.REGISTED_DATE>=$P{FromDate},RPD.ASSET_VALUE,"0.0")) AS  additions,
  SUM(IF(RPD.REGISTED_DATE<$P{FromDate},RPD.ASSET_VALUE,"0.0")) AS  balanceBF
FROM report_preparation_data AS RPD,
  asset,
  asset_location_details AS ald,
  location_master AS lm,
  section_master AS sm,
  department_master AS dm
WHERE asset.AS_CODE = RPD.ASSET_CODE
    AND asset.AS_ID = ald.AS_ID
    AND ald.IS_ACTIVE = '1'
    AND lm.LOC_ID = ald.LOC_ID
    AND lm.SEC_ID = sm.SEC_ID
    AND sm.DEP_ID = dm.DEP_ID
 AND dm.DEP_ID BETWEEN $P{fromcord}
    AND $P{tocord}
GROUP BY dm.DEP_ID]]></queryString>

