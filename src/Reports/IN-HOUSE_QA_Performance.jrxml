	<parameter name="list" class="java.util.List">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     avg(InsIhqaDet."IHQA_PassPt") AS InsIhqaDet_IHQA_PassPt,
     avg(InsIhqaDet."IHQA_FailPt") AS InsIhqaDet_IHQA_FailPt,
     avg(InsIhqaDet."IHQA_DefPt") AS InsIhqaDet_IHQA_DefPt,
     InsGeneralDet."Ins_Year" AS InsGeneralDet_Ins_Year,
     InsGeneralDet."Ins_Month" AS InsGeneralDet_Ins_Month,
     InsGeneralDet."Ins_Week" AS InsGeneralDet_Ins_Week,
     InsGeneralDet."Ins_Customer" AS InsGeneralDet_Ins_Customer,
     InsGeneralDet."Ins_Printer" AS InsGeneralDet_Ins_Printer
FROM
     "dbo"."InsGeneralDet" InsGeneralDet INNER JOIN "dbo"."InsIhqaDet" InsIhqaDet ON InsGeneralDet."Ins_Id" = InsIhqaDet."Ins_Id"
WHERE
     $X{ IN ,Ins_Year,list}
     and $X{ IN ,Ins_Month,list}
     and $X{ IN ,Ins_Week,list}
     and $X{ IN ,Ins_Printer,list}
GROUP BY
     InsGeneralDet.Ins_Year,
     InsGeneralDet.Ins_Month,
     InsGeneralDet.Ins_Week,
     InsGeneralDet.Ins_Customer,
     InsGeneralDet.Ins_Printer
ORDER BY
     InsGeneralDet.Ins_Week;]]>
	</queryString>
